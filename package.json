{
  "name": "chomp-test",
  "version": "1.0.0",
  "description": "React, Nest JS test application for chomp test",
  "author": "Jayden Jin",
  "private": true,
  "devDependencies": {
    "concurrently": "^4.1.1"
  },
  "scripts": {
    "server:dev": "cd server && npm run start:dev",
    "client:dev": "cd client && npm start",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:build": "cd server && npm run build",
    "client:build": "cd client && npm run build",
    "build": "concurrently \"npm run server:build\" \"npm run client:build\"",
    "server:start": "cd server && npm run start",
    "client:start": "cd client && npm start",
    "start": "concurrently \"npm run server:start\" \"npm run client:start\""
  }
}
